using System;
namespace optionalresulttest;

public static partial class Result
{
    public static T UnwrapOrElse<T, TErr>(this IResult<T, TErr> r, Func<TErr, T> f)
    {
        if(r.IsOk())
        {
            r.TryGet(out var ret);
            return ret;
        }
        else
        {
            r.TryGetError(out var er);
            return f(er);
        }
    }
